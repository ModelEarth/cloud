# Environment Variables Template
# Copy this file to .env and fill in your actual values
# DO NOT commit .env to version control

# Google Cloud Project Configuration
GOOGLE_CLOUD_PROJECT=your-project-id
GOOGLE_CLOUD_REGION=us-central1

# GitHub Configuration (private token only)
GITHUB_TOKEN=placeholder-token-needs-user-input

# Billing Account (for new projects)
BILLING_ACCOUNT_ID=000000-000000-000000

# Service Account Email (auto-generated during setup)
SERVICE_ACCOUNT_EMAIL=notebook-executor-sa@your-project-id.iam.gserviceaccount.com

# Cloud Run Service URL (set after deployment)
CLOUD_RUN_URL=https://your-service-url.run.app

# Optional: Custom Docker image registry
DOCKER_REGISTRY=gcr.io
IMAGE_TAG=latest

FLASK_ENV=dev #never has to be changed, in prod we don't use .env file
COLLABORATOR_EMAILS=collaborator1@example.com,collaborator2@example.com

# Auth config (auto-generated by rotate-token.sh)
UI_ACCESS_TOKEN=dummyvaluetobereplaced
